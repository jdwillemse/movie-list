{
  "name": "ssr-create-react-app-v2",
  "version": "0.1.0",
  "private": true,
  "url": "https://movie-lib.herokuapp.com/",
  "engines": {
    "node": "8.4.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "precommit",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "start:server": "NODE_ENV=development nodemon server/index.js",
    "now-start": "NODE_ENV=production nodemon server/index.js",
    "build": "react-scripts build && rm ./build/index.html",
    "test": "react-scripts test --env=jsdom",
    "heroku": "heroku local web",
    "heroku:deploy": "git push heroku master && heroku open",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "nodemon": "^1.12.0",
    "prettier": "^1.7.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.15.2",
    "firebase": "^4.3.1",
    "ignore-styles": "^5.0.1",
    "morgan": "^1.8.1",
    "node-fetch": "^1.7.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
